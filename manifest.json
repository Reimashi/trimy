{
    "manifest_version": 2,
    "name": "TriMY (alpha)",
    "version": "0.1.0.83",
    "default_locale": "en",
    "description": "__MSG_appDescription__",
    "minimum_chrome_version": "23",
    "icons": {
        "16": "lib/img/icon_16.png",
        "48": "lib/img/icon_48.png",
        "64": "lib/img/icon_64.png",
        "128": "lib/img/icon_128.png"
    },
    "app": {
        "launch": {
            "local_path": "app.html"
        }
    },
    "offline_enabled": false,
    "permissions": ["downloads", "<all_urls>"]
}